@model Shopping.Data.Entities.State

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Esta seguro que desea eliminar el item?</h3>
<div>
    <partial name="_StateDetails" />

    <form asp-action="DeleteState">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" asp-route-id="@Model?.Country.Id"class="btn btn-outline-success">Volver</a>
    </form>
</div>
